{
  "name": "json-view-chrome-plugin",
  "description": "谷歌浏览器扩展，用于格式化json，方便查看。",
  "license": "MIT",
  "scripts": {
    "dev": "npm run clean:dist && cross-env NODE_ENV=development gulp",
    "build": "npm run clean:dist && cross-env NODE_ENV=production gulp",
    "clean:dist": "rimraf ./dist",
    "eslint": "eslint ./ --fix",
    "update": "milktea update"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-json-view": "^1.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@sweet-milktea/milktea-cli": "^0.6.7",
    "@sweet-milktea/util-tools": "^0.7.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-yaml": "^2.0.4",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "pug": "^2.0.4",
    "rimraf": "^3.0.0",
    "url-loader": "^3.0.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.41.2",
    "webpack-chain": "^6.0.0",
    "webpack-stream": "^5.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
