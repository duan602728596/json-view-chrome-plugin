    "node-sass": "^4.12.0",
    "pug": "^2.0.4",
    "rimraf": "^2.6.3",
    "url-loader": "^2.1.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.38.0",
    "webpack-chain": "^6.0.0",
    "webpack-stream": "^5.2.1"
